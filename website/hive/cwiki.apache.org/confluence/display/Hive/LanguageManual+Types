><td class="confluenceTd"><p>false</p></td><td class="confluenceTd"><p>false</p></td><td class="confluenceTd"><p>false</p></td></tr><tr><th class="confluenceTh"><p>int to</p></th><td class="confluenceTd"><p>false</p></td><td class="confluenceTd"><p>false</p></td><td class="confluenceTd"><p>false</p></td><td class="confluenceTd"><p>false</p></td><td class="confluenceTd"><p>true</p></td><td class="confluenceTd"><p>true</p></td><td class="confluenceTd"><p>true</p></td><td class="confluenceTd"><p>true</p></td><td class="confluenceTd"><p>true</p></td><td class="confluenceTd"><p>true</p></td><td class="confluenceTd"><p>true</p></td><td class="confluenceTd"><p>false</p></td><td class="confluenceTd"><p>false</p></td><td class="confluenceTd"><p>false</p></td></tr><tr><th class="confluenceTh"><p>bigint to</p></th><td class="confluenceTd"><p>false</p></td><td class="confluenceTd"><p>false</p></td><td class="confluenceTd"><p>false</p></td><td class="confluenceTd"><p>false</p></td><td class="confluenceTd"><p>false</p></td><td class="confluenceTd"><p>true</p></td><td class="confluenceTd"><p>true</p></td><td class="confluenceTd"><p>true</p></td><td class="confluenceTd"><p>true</p></td><td class="confluenceTd"><p>true</p></td><td class="confluenceTd"><p>true</p></td><td class="confluenceTd"><p>false</p></td><td class="confluenceTd"><p>false</p></td><td class="confluenceTd"><p>false</p></td></tr><tr><th class="confluenceTh"><p>float to</p></th><td class="confluenceTd"><p>false</p></td><td class="confluenceTd"><p>false</p></td><td class="confluenceTd"><p>false</p></td><td class="confluenceTd"><p>false</p></td><td class="confluenceTd"><p>false</p></td><td class="confluenceTd"><p>false</p></td><td class="confluenceTd"><p>true</p></td><td class="confluenceTd"><p>true</p></td><td class="confluenceTd"><p>true</p></td><td class="confluenceTd"><p>true</p></td><td class="confluenceTd"><p>true</p></td><td class="confluenceTd"><p>false</p></td><td class="confluenceTd"><p>false</p></td><td clas